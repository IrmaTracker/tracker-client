{
  "name": "tracker-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/material-ui": "^0.18.0",
    "@types/node": "^8.0.28",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "material-ui": "^0.19.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts-ts": "2.6.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "cleanup": "rm -rf build",
    "deploy": "yarn cleanup && yarn build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --tab-width=4 --write",
      "git add"
    ],
    "*.tsx": [
      "prettier --tab-width=4 --write",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "prettier": "^1.6.1"
  }
}
